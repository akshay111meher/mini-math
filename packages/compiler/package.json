{
  "name": "@mini-math/compiler",
  "version": "0.1.0",
  "description": "Compiler",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:types": "tsc -p tsconfig.build.json",
    "build:js": "tsup src/index.ts --format esm,cjs",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "clean": "rimraf dist",
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@mini-math/logger": "^0.1.0",
    "@mini-math/nodes": "^0.1.0",
    "uuid": "^13.0.0",
    "@coinbase/cdp-sdk": "^1.36.0",
    "viem": "^2.33.2",
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.3.0"
  }
}