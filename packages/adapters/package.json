{
  "name": "@mini-math/adapters",
  "version": "0.1.0",
  "description": "Adapters",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:types": "tsc -p tsconfig.build.json",
    "build:js": "tsup src/index.ts --format esm,cjs",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "clean": "rimraf dist",
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write .",
    "schema:generate": "drizzle-kit generate",
    "db:push": "dotenv -e .env -- drizzle-kit push"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/node": "^22.7.5",
    "@types/pg": "^8.15.6",
    "@types/redis": "^4.0.10",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.7",
    "tsup": "^8.3.0"
  },
  "dependencies": {
    "@mini-math/keystore": "^0.1.0",
    "@mini-math/logger": "^0.1.0",
    "@mini-math/nodes": "^0.1.0",
    "@mini-math/queue": "^0.1.0",
    "@mini-math/rbac": "^0.1.0",
    "@mini-math/runtime": "^0.1.0",
    "@mini-math/workflow": "^0.1.0",
    "amqplib": "^0.10.9",
    "drizzle-orm": "^0.44.7",
    "pg": "^8.16.3",
    "redis": "^5.9.0"
  }
}
