{
  "name": "@mini-math/account-abstraction",
  "version": "0.1.0",
  "description": "Account Abstraction Contracts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:types": "tsc -p tsconfig.build.json",
    "build:js": "tsup typechain/index.ts --format esm,cjs",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "clean": "rimraf dist",
    "test": "vitest run",
    "lint": "echo \"(lint skipped)\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write .",
    "compile": "hardhat compile"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.3",
    "@nomicfoundation/hardhat-ignition": "^0.15.16",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.17",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.7.5",
    "chai": "^4.5.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "hardhat": "^2.28.3"
  },
  "devDependencies": {
    "hardhat-gas-reporter": "^2.3.0",
    "solidity-coverage": "^0.8.17",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "typechain": "^8.3.2"
  }
}
